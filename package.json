{
  "name": "biome-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "biome analyze --config-path ./node_modules/@tooling/biome/root.json",
    "build": "pnpm -r build",
    "clean": "git clean -dfx .turbo node_modules && pnpm -r clean",
    "format": "biome format --config-path ./node_modules/@tooling/biome/root.json",
    "start:dev": "pnpm -r --parallel --stream start:dev"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@tooling/biome": "workspace:",
    "turbo": "^2.4.4"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=22.14.0",
    "npm": "DO NOT USE NPM",
    "pnpm": "10.7.0",
    "yarn": "DO NOT USE YARN"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@nestjs/core",
      "@swc/core"
    ],
    "ignoredBuiltDependencies": [
      "@nestjs/core"
    ]
  }
}
